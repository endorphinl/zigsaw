{
    "logs" : [
    <% @log_array.each_with_index do |line, index| %>
        <% if !line.nil? %>
            <% log_elements = line.split("_") %>
            {
                "action_type": "<%= log_elements[0] %>",
                "file_name": "<%= log_elements[1] %>",
                "chunk": "<%= log_elements[2] %>",
                "account": "<%= log_elements[0] == 'donate' || log_elements[0] == 'revoke' ? log_elements[1] : log_elements[3] %>",
                "upload_type": "<%= log_elements[4] %>"
            }<% if index != @log_array.size - 1 %>,<% end %>
        <% end %>
    <% end %>
    ]
}
